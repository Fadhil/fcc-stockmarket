{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
      "comma-dangle": [0],
      "react/prefer-stateless-function": [0],
      "global-require": [0],
      "no-console": [0],
      "strict": [0],
      "no-underscore-dangle": [0],
      "react/prop-types": [0]
    }
}
