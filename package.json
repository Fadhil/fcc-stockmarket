{
  "name": "stockmarket",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/index.js",
  "scripts": {
    "eslint": "eslint",
    "mocha": "mocha",
    "istanbul": "istanbul",
    "codeclimate": "codeclimate-test-reporter < coverage/combined/lcov.info",
    "coverage:server": "better-npm-run coverage:server",
    "coverage:combine": "istanbul report --root coverage/ --dir coverage/combined lcov",
    "start": "node src/server/index.js",
    "dev:server": "better-npm-run dev:server",
    "dev:client": "better-npm-run dev:client",
    "dev:client:hot": "better-npm-run dev:client:hot",
    "dev:hot": "better-npm-run dev:hot",
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "test": "npm run test:client && npm run test:server",
    "test:client": "better-npm-run test:client",
    "test:client:watch": "better-npm-run test:client:watch",
    "test:server": "better-npm-run test:server",
    "test:server:watch": "better-npm-run test:server:watch",
    "tdd:client": "better-npm-run tdd:client",
    "tdd:client:hot": "better-npm-run tdd:client:hot",
    "tdd:server": "better-npm-run tdd:server",
    "heroku-postbuild": "npm run build"
  },
  "betterScripts": {
    "coverage:server": {
      "command": "istanbul cover -x **/*.func.js -x **/*.spec.js --dir ./coverage/server --report lcov node_modules/mocha/bin/_mocha \"src/server/**/*.spec.js\"",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "dev:server": {
      "command": "nodemon src/server/index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:client": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:client:hot": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development",
        "HMR": "enabled"
      }
    },
    "dev": {
      "command": "concurrent -k \"npm run dev:server\" \"npm run dev:client\""
    },
    "dev:hot": {
      "command": "concurrent -k \"npm run dev:server\" \"npm run dev:client:hot\""
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test:client": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:client:watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:server": {
      "command": "mocha \"src/server/**/*.spec.js\"",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:server:watch": {
      "command": "mocha -w \"src/server/**/*.spec.js\"",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "tdd:client": {
      "command": "concurrent -k \"npm run dev:client\" \"npm run test:client:watch\""
    },
    "tdd:client:hot": {
      "command": "concurrent -k \"npm run dev:client:hot\" \"npm run test:client:watch\""
    },
    "tdd:server": {
      "command": "concurrent -k --raw \"npm run dev:server\" \"npm run test:server:watch\""
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jomcode/fcc-stockmarket.git"
  },
  "author": "John <jomcode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jomcode/fcc-stockmarket/issues"
  },
  "homepage": "https://github.com/jomcode/fcc-stockmarket#readme",
  "engines": {
    "node": ">=6.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "codeclimate-test-reporter": "^0.3.3",
    "concurrently": "^2.2.0",
    "css-loader": "^0.25.0",
    "dotenv": "^2.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.7.0",
    "nodemon": "^1.10.2",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.1",
    "sass-loader": "^3.2.0",
    "sinon": "^2.0.0-pre.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "chart.js": "^1.1.1",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "feathers": "^2.0.1",
    "feathers-configuration": "^0.3.1",
    "feathers-errors": "^2.4.0",
    "feathers-hooks": "^1.5.7",
    "feathers-socketio": "^1.4.1",
    "got": "^6.3.0",
    "moment": "^2.14.1",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-chartjs": "^0.7.3",
    "react-dom": "^15.3.1",
    "rxjs": "^5.0.0-beta.12",
    "socket.io-client": "^1.4.8",
    "winston": "^2.2.0"
  }
}
